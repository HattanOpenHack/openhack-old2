pipeline{
      agent  { node { label 'master' } }

stages
{
    stage('checkout')
    {
        steps
        {
            deleteDir()
        }
    }
    stage('setup')
    {
        steps
        {
            echo "ACR Setup"    
        }
    }
    stage('build')
    {
        steps
        {
            echo "build"
        }
    }
}

}