pipeline{
      agent  { node { label 'master' } }

stages
{
    stage('clone')
    {
        steps
        {

            checkout([$class: 'GitSCM', branches: [[name: '*/master']],
            userRemoteConfigs: [[url: 'https://github.com/HattanOpenHack/openhack-devops-team.git/']],credentialsId: 'hattan-pat'])
        }
    }
    stage('setup')
    {
        steps
        {
            sh "ls" 
        }
    }
    stage('build')
    {
        steps
        {
            sh "pwd"
        }
    }
}

}